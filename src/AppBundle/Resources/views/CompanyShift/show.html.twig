{% extends '::base.html.twig' %}

{% block body -%}
    <h1>CompanyShift</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Companyid</th>
                <td>{{ entity.companyId }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{{ entity.start }}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{{ entity.end }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('manager_company_shifts') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('manager_company_shifts_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
